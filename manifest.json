{
  "name": "Sanastorm",
  "version": "0.1",
  "description": "Get Finnish words info",
  "permissions": ["activeTab", "https://*/"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["content.css"],
      "js": ["./dist/contentScript.js"]
    }
  ],
  "page_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/finland16.png",
    "32": "images/finland32.png",
    "64": "images/finland64.png",
    "128": "images/finland128.png"
  },
  "web_accessible_resources": ["images/finland32.png", "images/wikt.png"],
  "manifest_version": 2
}
